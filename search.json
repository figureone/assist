---
layout: null
---
{
  {% for post in site.posts %}

    "{{ post.url | slugify }}": {
      "title": "{{ post.title | xml_escape }}",
      "url": " {{ post.url | prepend:site.baseurl | xml_escape }}",
      "tags": "{{ post.tags | xml_escape }}",
      "category": "{{ post.category | xml_escape }}"
    }
    {% unless forloop.last %},{% endunless %}
  {% endfor %}
}